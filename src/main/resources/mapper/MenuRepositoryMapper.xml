<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.teamBus.web.repository.MenuRepository">
	<select id="findAll" resultType="com.teamBus.web.entity.Menu">
		select *
		from menu
	</select>
	<insert id="addMenu" parameterType="com.teamBus.web.entity.Menu">
		insert into menu(name, price)
		values (#{name}, #{price});
	</insert>
